<p id="notice"><%= notice %></p>

 <div class="page-header">
  <h1 class="well well-lg profile-text"> Projects </h1>
</div><br><br>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Project Number</th>
      <th scope="col">Project Name</th>
      <th colspan="3", scope="col"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr scope="row">
        <td><%= project.p_number %></td>
        <td><%= project.p_name %></td>
        <td><%= link_to 'Show', project, class: "btn btn-secondary" %></td>
        <td><%= link_to 'Edit', edit_project_path(project), class: "btn btn-secondary" %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-secondary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path, class: "btn btn-primary" %>

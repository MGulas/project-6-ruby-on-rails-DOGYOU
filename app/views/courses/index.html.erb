<p id="notice"><%= notice %></p>

 <div class="page-header">
  <h1 class="well well-lg profile-text"> Courses </h1>
</div><br><br>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Section</th>
      <th scope="col">Section Name</th>
      <th colspan="3", scope="col"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr scope="row">
        <td><%= course.course_id %></td>
        <td><%= course.course_name %></td>
        <td><%= link_to 'Show', course, class: "btn btn-secondary" %></td>
        <td><%= link_to 'Edit', edit_course_path(course), class: "btn btn-secondary" %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-secondary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path, class: "btn btn-primary" %>
